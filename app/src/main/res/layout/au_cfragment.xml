<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AU_CFragment">
    <TextView
        android:id="@+id/tv4"
        android:layout_width="wrap_content"
        android:layout_height="50sp"
        android:layout_above="@+id/tv5"
        android:layout_centerHorizontal="true"
        android:textSize="20sp"
        android:gravity="center"
        android:textColor="@color/color_white"
        android:text="请向我们提出您宝贵的意见！点击号码即可拨打电话" />

    <TextView
        android:id="@+id/tv5"
        android:layout_width="wrap_content"
        android:layout_height="50sp"
        android:layout_centerVertical="true"
        android:layout_above="@+id/telephone1"
        android:layout_centerHorizontal="true"
        android:textSize="20sp"
        android:textColor="@color/color_white"
        android:text=" " />

    <Button
        android:id="@+id/telephone1"
        android:layout_width="wrap_content"
        android:layout_height="50sp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:background="@color/transparent"
        android:textSize="40sp"
        android:textColor="@color/color_white"
        android:text="15692329128" />
    <TextView
        android:id="@+id/tv1"
        android:layout_width="wrap_content"
        android:layout_height="50sp"
        android:layout_centerVertical="true"
        android:layout_toLeftOf="@+id/telephone1"
        android:textSize="20sp"
        android:textColor="@color/color_white"
        android:text="成员一" />

    <Button
        android:id="@+id/telephone3"
        android:layout_width="wrap_content"
        android:layout_height="50sp"
        android:layout_centerInParent="true"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/telephone1"
        android:background="@color/transparent"
        android:textSize="40sp"
        android:textColor="@color/color_white"
        android:text=" " />
    <TextView
        android:id="@+id/tv3"
        android:layout_width="wrap_content"
        android:layout_height="50sp"
        android:layout_below="@+id/tv1"
        android:layout_toLeftOf="@+id/telephone3"
        android:layout_centerInParent="true"
        android:textSize="40sp"
        android:textColor="@color/color_white"
        android:text=" " />

    <Button
        android:id="@+id/telephone2"
        android:layout_width="wrap_content"
        android:layout_height="50sp"
        android:layout_centerInParent="true"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/telephone3"
        android:background="@color/transparent"
        android:textSize="40sp"
        android:textColor="@color/color_white"
        android:text="13061235982" />
    <TextView
        android:id="@+id/tv2"
        android:layout_width="wrap_content"
        android:layout_height="50sp"
        android:layout_below="@+id/tv3"
        android:layout_toLeftOf="@+id/telephone2"
        android:layout_centerInParent="true"
        android:textSize="20sp"
        android:textColor="@color/color_white"
        android:text="成员二" />
</RelativeLayout>